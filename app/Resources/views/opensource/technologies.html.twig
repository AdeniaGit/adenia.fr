{% block javascripts %}
    <script src="{{ asset('assets/js/onepager/jquery.pagescroller.lite.js') }}" type="text/javascript"></script>
    <script src="{{ asset('assets/js/onepager/init.js') }}" type="text/javascript"></script>

    <script  type="text/javascript">

        $(function(){

            var origin = $('#FixedDiv').offset().top;
            $(window).scroll(function() {
                var top  = $(this).scrollTop();
                if(top>origin)
                    $('#FixedDiv').animate({top:$(this).scrollTop()-origin+20}, 25, "linear");
                else
                    $('#FixedDiv').animate({top:$("#articlePhp").scrollTop()}, 25, "linear");
            });
        })

    </script>
